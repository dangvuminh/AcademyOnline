m\"\nimport Axios from \"axios\"\nimport avatar from \"../../../img/avatar.png\"\nimport \"../../../css/student-home/student-home.css\"\nimport OwnLesson  from \"../lesson/ownLesson\"\nimport FavoriteLesson from \"../lesson/favoriteLesson\"\n\n\nexport default function StudentHome() {\n    const [student, setStudent] = useState([]);\n    const [lesson,setLesson] = useState([]);\n    const [isOwn,setIsOwn] = useState(false);\n    const [isFavorite,setIsFavorite] = useState(false);\n    const [msg,setMsg] = useState(\"\");\n    const params = useParams();\n    const username = params.username;\n\n    useEffect(() => {\n        Axios({\n            method: \"post\",\n            url: `http://localhost:4000/api/signIn/getStudentProfile/${username}`,\n            data: {\n                accessToken: localStorage.getItem('accessToken'),\n            }\n        }).then((result) => {\n            setStudent(result.data[0]);\n        }).catch(err => {\n            alert(\"Your Log-in time has run out.Please Log in again!!\");\n            localStorage.setItem('isLogin', false);\n            window.open(\"/\", \"_parent\");\n        })\n    }, []);\n\n    const getOwnCourseList = () => {\n        Axios({\n            method: \"post\",\n            url: `http://localhost:4000/api/getEnrollment`,\n            data: {\n                studentID: student.student_id,\n                accessToken: localStorage.getItem('accessToken'),\n            }\n        }).then(result => {\n            if (result.data.state == 0){\n                setMsg(\"No course Available right now\");\n                    setIsFavorite(false);\n            }\n                else{\n                    console.log(result.data);\n                    setLesson(result.data);\n                    setIsOwn(true);\n                    setIsFavorite(false);\n                    setMsg(\"\");\n                }\n               \n        }).catch(err => {\n            //if (err) throw err;\n            alert(\"Your Log-in time has run out.Please Log in again!!\");\n            localStorage.setItem('isLogin', false);\n            window.open(\"/\", \"_parent\");\n        })\n    }\n\n   const getFavoriteCourseList = () =>{\n    Axios({\n        method: \"post\",\n        url: `http://localhost:4000/api/getFavoriteCourseList`,\n        data: {\n            studentID: student.student_id,\n            accessToken: localStorage.getItem('accessToken'),\n        }\n    }).then(result => {\n        if (result.data.state == 0){\n            setMsg(\"No Favorite Course \");\n            setIsOwn(false);\n        }\n            else{\n                console.log(result.data);\n                setLesson(result.data);\n                setIsFavorite(true);\n                setIsOwn(false);\n                setMsg(\"\");\n            }\n           \n    }).catch(err => {\n        //if (err) throw err;\n        alert(\"Your Log-in time has run out.Please Log in again!!\");\n        localStorage.setItem('isLogin', false);\n        window.open(\"/\", \"_parent\");\n    })\n   }\n\n    const drawLesson=()=>{\n        if(isOwn == true){\n            return <OwnLesson lesson={lesson}/>\n        } else if(isFavorite ==true){\n            return <FavoriteLesson lesson={lesson}/>\n        }\n    }\n\n    const drawStudentHome = () => {\n        return <div className=\"student-home\">\n\n            <div className=\"student-home-profile\" className=\"card\" style={{ width: '300px' }}>\n                <img className=\"card-img-top\" src={avatar} alt=\"Card image\" />\n                <div className=\"card-body\">\n                    <h4 className=\"card-title\">{student.student_firstname} {student.student_lastname}</h4>\n                    <p className=\"card-text\">Some example text.</p>\n                    <a href=\"#\" className=\"btn btn-primary\">Edit Profile</a>\n                </div>\n            </div>\n\n            <div className=\"student-home-course-manage\">\n\n                <div className=\"student-home-category\">\n                    <div onClick={()=>{ getFavoriteCourseList() }} className=\"favorite student-home-item\">\n                        <h2>Your Favourite Courses</h2>\n                        <i class=\"fa fa-heart\"></i>\n                    </div>\n                    <div onClick={() => { getOwnCourseList() }} className=\"boughtCourses student-home-item\">\n                        <h2>Your Own Courses</h2>\n                        <i class=\"fa fa-book\"></i>\n                    </div>\n                </div>\n                <div className=\"student-home-course-list\">\n                <h2 style={{marginTop:\"25px\"}} className=\"msg\">{msg}</h2>\n                {drawLesson()}\n                </div>\n            </div>\n\n        </div>\n\n    }\n\n    return (\n        <div >\n            {drawStudentHome()}\n\n        </div>\n    )\n}\n","/Users/macbookpro/Desktop/1653011_AcademyOnlineProject/AcademyOnline/frontend/src/bar/student-home/lesson/ownLesson.js",[],"/Users/macbookpro/Desktop/1653011_AcademyOnlineProject/AcademyOnline/frontend/src/bar/student-home/lesson/favoriteLesson.js",[],{"ruleId":"95","replacedBy":"96"},{"ruleId":"97","replacedBy":"98"},{"ruleId":"99","severity":1,"message":"100","line":2,"column":17,"nodeType":"101","messageId":"102","endLine":2,"endColumn":27},{"ruleId":"103","severity":1,"message":"104","line":26,"column":8,"nodeType":"105","endLine":26,"endColumn":10,"suggestions":"106"},{"ruleId":"103","severity":1,"message":"104","line":43,"column":8,"nodeType":"105","endLine":43,"endColumn":10,"suggestions":"107"},{"ruleId":"108","severity":1,"message":"109","line":54,"column":34,"nodeType":"110","messageId":"111","endLine":54,"endColumn":36},{"ruleId":"112","severity":1,"message":"113","line":55,"column":44,"nodeType":"114","messageId":"115","endLine":55,"endColumn":46},{"ruleId":"108","severity":1,"message":"116","line":56,"column":41,"nodeType":"110","messageId":"111","endLine":56,"endColumn":43}