                            <div>\n                                <h3>Assign Teacher</h3>\n                                <p style={{color:\"red\"}}>{err2}</p>\n                                {getTeacherList()}\n                            </div>\n\n                            <button type=\"submit\" className=\"btn btn-primary\">Create</button>\n                        </form>\n        </div>\n    )\n}\n","/Users/macbookpro/Desktop/1653011_AcademyOnlineProject/AcademyOnline/frontend/src/components/admin/teacher/createTeacher.js",["234"],"import React,{useState} from 'react'\nimport Axios from \"axios\"\n\nexport default function CreateTeacher() {\n    const [username,setUsername] = useState(\"\");\n    const [password1,setPassword1] = useState(\"\");\n    const [password2,setPassword2] = useState(\"\");\n    const [err1,setErr1] = useState(\"\");\n    const [err2,setErr2] = useState(\"\");\n\n    const submitHandle = (e) => {\n        e.preventDefault();\n\n        if(password1 != password2){\n            setErr2(\"The passwords don't match.Please type again!!\")\n        } else{\n            Axios({\n                method:\"post\",\n                url:\"http://localhost:4000/api/createTeacher\",\n                data:{\n                    username : username,\n                    password: password1\n                }\n            }).then(result=>{\n                if(result.data.state === 1){\n                    alert(\"Created a teacher account!!\");\n                } else{\n                    setErr1(\"This username has been used already!!\");\n                }\n            })\n        }    \n    }\n\n    return (\n        <div>\n            <h3>Create Teacher Account</h3>\n            <form onSubmit={submitHandle} className=\"was-validated\">\n        <div className=\"form-group\">\n          <label htmlFor=\"uname\">Username:</label>\n          {/* Error msg */}\n          <p style={{color:\"red\"}}>{err1}</p>\n          <input onChange={(e)=>{setUsername(e.target.value)}}\n          type=\"text\" className=\"form-control\" id=\"uname\" placeholder=\"Enter username\" required />\n          <div className=\"valid-feedback\">Valid.</div>\n          <div className=\"invalid-feedback\">Please fill out this field.</div>\n        </div>\n\n        <div className=\"form-group\">\n          <label htmlFor=\"pwd\">Password:</label>\n          <input onChange={(e)=>{setPassword1(e.target.value)}}\n          type=\"password\" className=\"form-control\" id=\"pwd\" placeholder=\"Enter password\" required />\n          <div className=\"valid-feedback\">Valid.</div>\n          <div className=\"invalid-feedback\">Please fill out this field.</div>\n        </div>\n\n        <div className=\"form-group\">\n          <label htmlFor=\"pwd\">Enter Password Again:</label>\n          {/* Error msg */}\n          <p style={{color:\"red\"}}>{err2}</p>\n          <input onChange={(e)=>{setPassword2(e.target.value)}}\n          type=\"password\" className=\"form-control\" id=\"pwd\" placeholder=\"Enter password again\"  required />\n          <div className=\"valid-feedback\">Valid.</div>\n          <div className=\"invalid-feedback\">Please fill out this field.</div>\n        </div>\n\n        <button type=\"submit\" className=\"btn btn-primary\">Submit</button>\n      </form>\n        </div>\n    )\n}\n","/Users/macbookpro/Desktop/1653011_AcademyOnlineProject/AcademyOnline/frontend/src/components/teacher/authen/authenTeacherRoute.js",["235"],"import React from 'react'\nimport {Route,Redirect} from \"react-router-dom\"\n\nexport default function AuthenTeacherRoute({ component: C, ...rest }) {\n\n    return (\n        <Route\n        {...rest}\n        render={props =>\n          localStorage.getItem(\"teacherLogIn\") == \"true\"\n            ? <C {...props} />\n            : <Redirect\n                to={`/notfound`}\n              />\n            }\n      />\n    )\n}\n",["236","237"],"/Users/macbookpro/Desktop/1653011_AcademyOnlineP